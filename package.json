  "description": "KnowledgeGas AI - поисковик-лендинг для базы знаний ТЭК",
  "type": "module",
  "homepage": "https://zmrdcz.github.io/DataGazCore/",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:gh-pages": "cross-env NODE_ENV=production vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-index": "tsx scripts/build_index.ts",
    "setup-demo": "node scripts/setup_demo.js",
    "deploy": "npm run build:gh-pages && gh-pages -d dist"
  },
  "dependencies": {
    "@types/three": "^0.162.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/motion": "^2.0.0",
    "axios": "^1.6.7",
    "fuse.js": "^7.0.0",
    "gsap": "^3.12.5",
    "pinia": "^2.1.7",
    "three": "^0.162.0",
    "tsx": "^4.7.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@storybook/addon-controls": "^8.0.5",
    "@storybook/addon-docs": "^8.0.5",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/vue3": "^8.0.5",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "gh-pages": "^6.3.0",
    "jsdom": "^24.0.0",
    "playwright": "^1.42.1",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "storybook": "^8.0.5",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6"
